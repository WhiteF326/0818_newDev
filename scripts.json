{
  "$schema": "https://deno.land/x/denon@2.4.7/schema.json",
  "scripts": {
    "start": {
      "cmd": "deno run --unstable -A server.js",
      "desc": "run my app.ts file"
    }
  },
  "watcher": {
    "interval": 350,
    "exts": ["js", "jsx", "ts", "tsx"],
    "match": ["**/*.*"],
    "skip": ["*/.git/*"],
    "legacy": false
  }
}