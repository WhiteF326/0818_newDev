import { fetchJSON } from 'https://js.sabae.cc/fetchJSON.js';

let moving = false;

window.onload = async () => {
  
};

document.getElementById('move').onclick = () => {
  if (moving) {
    moving = false;
    document.getElementById('mode').innerText = '停止中';
  } else {
    moving = true;
    document.getElementById('mode').innerText = '移動中';
  }
};
